program miprograma {
    var x;
    var y;
    var z;
    var contador;
    var i;

    haz x 6;
    haz y 2;
    SI (x<y)
    [println x;]
    [println y;]

    haz contador 0;
    HAZ.HASTA (contador < 3) [
        println contador;
        haz contador contador + 1;
    ]

    haz i 10;
    HAZ.HASTA (i > 5) [
        println i;
        haz i i - 1;
    ]
}
