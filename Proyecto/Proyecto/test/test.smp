program complejo {
    var a;
    var b;
    var c;
    var contador;
    var i;

    haz a 5;
    haz b 3;
    haz c 0;

    INC [a 2];
    INC [b];
    INC [c a];

    SI (a > b)
    [
        haz c c + b;
        INC [b 2];
    ]
    [
        haz c c - b;
        INC [a];
    ]

    haz contador 0;
    HAZ.HASTA (contador < 4) [
        println contador;
        haz contador contador + 1;
        INC [a];
        INC [b 2];
    ]

    haz i 7;
    HAZ.HASTA (i > 0) [
        println i;
        haz i i - 2;
        INC [c 3];
    ]

    println a;
    println b;
    println c;
}
